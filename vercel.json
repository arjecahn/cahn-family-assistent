{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/push/morning-reminders",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/push/evening-reminders",
      "schedule": "0 17 * * *"
    }
  ]
}
